<?xml version="1.0" encoding="UTF-8"?>
        <!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="com.example.springbootdemo.dao.mapper.system.systemUserMapper">
    <select id="getAllSystemUser"  resultType="systemUser">
        SELECT *
        from system_user ;
    </select>

    <select id="getSystemUserByLoginName">
        select * from system_user where login_name=#{loginName}
    </select>
</mapper>
